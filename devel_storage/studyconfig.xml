  <sc:macrodef id="spreadsheet-instructions-page">
    <sc:page>
      <sc:name>instructions</sc:name>
      <sc:instructions>
        <html>
          <sc:macro id="param-banner"/>
          <b>Instructions:</b>
          <ol>
            <li>Click "Open Sample Document" below.</li>
            <li>Edit the spreadsheet that opened in a different window<br/>
                (according to the instructions in the HIT).</li>
            <li>Save and close the spreadsheet.</li>
            <li>Click "Upload and Retrieve Confirmation Code" below.</li>
            <li>Paste the confirmation code into the HIT.</li>
          </ol>
          <p align="right">
            Thanks!<br/>
            <a href="mailto:ebakke@mit.edu">ebakke@mit.edu</a>
          </p>
        </html>
      </sc:instructions>
      <sc:openfile>
        <sc:buttontext mnemonic="D">Open Sample Document</sc:buttontext>
        <sc:servername><sc:macro id="param-servername"/></sc:servername>
        <sc:clientname><sc:macro id="param-clientname"/></sc:clientname>
        <sc:errormessage><sc:macro id="param-errormessage"/></sc:errormessage>
      </sc:openfile>
      <sc:conclude>
        <sc:buttontext mnemonic="U">Upload and Retrive Confirmation Code...</sc:buttontext>
        <sc:uploadfile>
          <sc:macro id="param-filefilter"/>
          <sc:default><sc:macro id="param-clientname"/></sc:default>
        </sc:uploadfile>
      </sc:conclude>
    </sc:page>
  </sc:macrodef>

