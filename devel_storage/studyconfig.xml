  <sc:macrodef id="filefilter-excel">
    <sc:filefilter>
      <sc:description>Microsoft Excel Workbook</sc:description>
      <sc:extension>xls</sc:extension>
      <sc:extension>xml</sc:extension>
      <sc:extension>xlsx</sc:extension>
      <sc:extension>xlsm</sc:extension>
      <sc:extension>xlsb</sc:extension>
    </sc:filefilter>
  </sc:macrodef>

  <sc:macrodef id="filefilter-relspread">
    <sc:filefilter>
      <sc:description>Spreadsheet Study Application files</sc:description>
      <sc:extension>rzw</sc:extension>
    </sc:filefilter>
  </sc:macrodef>

