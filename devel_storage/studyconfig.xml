  <sc:macrodef id="uistrings-english">
    <sc:uistrings>
      <sc:uistring key="DIALOG_CONSENT_TITLE">Screencasting</sc:uistring>
      <sc:uistring key="DIALOG_CONSENT_QUESTION"><html>
        This tool will record the contents of your screen while you perform the HIT,<br/>
        so we can learn about how different people approached the task.<br/><br/>
        OK to start recording?</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_ALREADY_RUNNING_TITLE">Already Running</sc:uistring>
      <sc:uistring key="DIALOG_ALREADY_RUNNING_MESSAGE">
        <html>The StudyCaster client is already running in a different window.<br/>
                       Please close it first if you wish to start over.</html>
      </sc:uistring>
      <sc:uistring key="MAINFRAME_BACK_BUTTON"       mnemonic="B">&lt; Back</sc:uistring>
      <sc:uistring key="MAINFRAME_NEXT_BUTTON"       mnemonic="N">Next &gt;</sc:uistring>
      <sc:uistring key="DIALOG_CLOSE_TITLE"  >Exit Without Uploading?</sc:uistring>
      <sc:uistring key="DIALOG_CLOSE_MESSAGE">
        If you exit the StudyCaster client without uploading first, your changes will be lost. OK to quit?
      </sc:uistring>
      <sc:uistring key="PROGRESS_OPEN_FILE">Opening file...</sc:uistring>
      <sc:uistring key="PROGRESS_OPEN_URI">Opening web page...</sc:uistring>
      <sc:uistring key="PROGRESS_UPLOAD_FILE">Uploading file...</sc:uistring>
      <sc:uistring key="PROGRESS_UPLOAD_SCREENCAST">Finishing screencast upload...</sc:uistring>

      <sc:uistring key="DIALOG_OPEN_FILE_TITLE">Open Sample Document</sc:uistring>
      <sc:uistring key="DIALOG_OPEN_FILE_EXISTING_MESSAGE"><html>
        The StudyCaster client found a modified file from an old session:<br/>
        {0}<br/><br/>
        Would you like to keep working on the existing file, or download a new one?</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_OPEN_FILE_MODIFIED_MESSAGE"><html>
        The file was previously opened and modified:<br/>
        {0}<br/><br/>
        Would you like to keep working on the existing file, or download a new one?</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_OPEN_FILE_RENAMED_MESSAGE"><html>
        The old file<br/>
        {0}<br/>
        was renamed to<br/>
        {1}</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_OPEN_FILE_RENAME_FAILED_MESSAGE"><html>
        Failed to rename the file<br/>
        {0}<br/><br/>
        If the file is still open, please close it and try again.</html>
      </sc:uistring>
      <!-- The message below is shown on a best-effort basis, currently never on JRE 1.5. The second
      message parameter is a string from the <sc:errormessage> element in the <sc:openfile> configuration.
      -->
      <sc:uistring key="DIALOG_OPEN_FILE_ASSOCIATION_FAILED_MESSAGE"><html>
        Failed to open the file<br/>{0}<br/><br/>
        {1}</html>
      </sc:uistring>
      <!-- The message below is shown on a best-effort basis; it is not always possible to detect
      whether a file is already open in a different program. -->
      <sc:uistring key="DIALOG_OPEN_FILE_ALREADY_MESSAGE"><html>
        The file already appears to be open:<br/>
        {0}<br/><br/>
        The file should have opened in a new window, possibly in the background.</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_OPEN_FILE_NEW_BUTTON">Download New File</sc:uistring>
      <sc:uistring key="DIALOG_OPEN_FILE_KEEP_BUTTON">Keep Existing File</sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_TITLE">Upload and Retrieve Confirmation Code</sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_FILE_PATH_LABEL" mnemonic="F">File to upload:</sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_FILE_INFO_LABEL">
        If you saved the file to a different location, select the new location here. Otherwise, just click OK.
      </sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_FILE_BROWSE_BUTTON" mnemonic="B">Browse...</sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_QUESTION">
        OK to conclude study and retrieve a confirmation code?
      </sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_FILE_NOT_OPENED_MESSAGE">
        To upload an edited file, please open it first using the "Open Sample Document" button.
      </sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_EMPTY_PATH_MESSAGE">
        Please select a file to upload.
      </sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_FILE_NOT_EXISTS_MESSAGE"><html>
        The file does not exist:<br/>
        {0}</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_FILE_OPEN_MESSAGE"><html>
        Please close the file, then try again.<br/>
        {0}</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_CONCLUDE_FILE_NOT_MODIFIED_MESSAGE"><html>
        Please edit, save, and close the file before uploading it:<br/>
        {0}<br/><br/>
        If necessary, open the file again using the "Open Sample Document" button.</html>
      </sc:uistring>
      <sc:uistring key="DIALOG_CONFIRMATION_TITLE">Upload Successful</sc:uistring>
      <sc:uistring key="DIALOG_CONFIRMATION_CODEBOX_LABEL" mnemonicindex="24">
        Upload Successful! Your confirmation code is...
      </sc:uistring>
      <sc:uistring key="DIALOG_CONFIRMATION_CLIPBOARD_MESSAGE">
        (The code has been copied to the clipboard so you can paste it into the HIT.)
      </sc:uistring>

    </sc:uistrings>
  </sc:macrodef>
