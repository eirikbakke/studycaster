<sc:macrodef id="layoutstudy-configuration">
  <sc:configuration>
    <sc:id><sc:macro id="param-id"/></sc:id>
    <sc:name>Layout Manager Study
    (<sc:macro id="param-layouttype1"/>-<sc:macro id="param-layouttype2"/>-<sc:macro id="param-layouttype3"/>)</sc:name>
    <sc:macro id="uistrings-english"/>
    <sc:screencast>
      <sc:whitelist>Acrobat</sc:whitelist>
      <sc:whitelist>Reader</sc:whitelist>
      <sc:whitelist>PDF</sc:whitelist>
      <sc:whitelist>userstudy</sc:whitelist>
      <sc:blacklist>Firefox</sc:blacklist>
      <sc:blacklist>Internet Explorer</sc:blacklist>
      <sc:blacklist>Chrome</sc:blacklist>
      <sc:blacklist>Safari</sc:blacklist>
      <sc:blacklist>Opera</sc:blacklist>
      <sc:blacklist>Thunderbird</sc:blacklist>
      <sc:blacklist>Outlook</sc:blacklist>
    </sc:screencast>

    <sc:macrodef id="layoutquestion-style">
      <style type="text/css">
        body {
          width: 280px;
        }
        .description {
          font-size: 15pt;
          margin-left: 12pt;
        }
        .varying {
          text-decoration: underline;
        }
        .dheading, h1 {
          font-weight: bold;
          font-size: 15pt;
          margin: 0px;
        }
        p {
          margin-bottom: 12pt;
        }
      </style>
    </sc:macrodef>

    <sc:macrodef id="layoutquestion-page">
      <sc:page>
        <sc:name>question<sc:macro id="param-number"/><sc:macro id="param-subletter"/></sc:name>
        <sc:instructions>
          <html>
            <sc:macro id="layoutquestion-style"/>
            <p>
              The question below is based on the PDF file you opened previously.
            </p>
            <p class="description">
              <sc:macro id="param-pdfdescription"/>
            </p>
            <p class="description">
              <span class="dheading">Question
              <sc:macro id="param-number"/><sc:macro id="param-subletter"/>:</span>
              <sc:macro id="param-question"/>
              (E.g. "<sc:macro id="param-answerexample"/>")
            </p>
            <p>
              Please type your answer into the HIT form in your browser, then click "Next" to continue to the next question.
            </p>
          </html>
        </sc:instructions>
      </sc:page>
    </sc:macrodef>

    <sc:macrodef id="layoutquestionset">
      <sc:page>
        <sc:name>question<sc:macro id="param-number"/>-instructions</sc:name>
        <sc:macrodef id="pdftitle">PDF File for Questions <sc:macro id="param-number"/>A and <sc:macro id="param-number"/>B</sc:macrodef>
        <sc:instructions>
          <html>
            <sc:macro id="layoutquestion-style"/>
            <p>If you have any PDF files currently open, please close them.</p>

            <p>Then, please click the button below to open the PDF file that will be used for the next two questions.</p>
            <p class="description">
              <span class="dheading">
              PDF File for Questions <sc:macro id="param-number"/>A and <sc:macro id="param-number"/>B:</span><br/>
              <sc:macro id="param-pdfdescription"/>
            </p>
            <p>When the PDF file has opened, click "Next" to proceed to the questions.</p>
          </html>
        </sc:instructions>
        <sc:openfile>
          <sc:buttontext mnemonic="O">Open <sc:macro id="pdftitle"/></sc:buttontext>
          <sc:servername>
            layoutstudy/<sc:macro id="param-corpuskey"/>_<sc:macro id="param-layouttype"/>.pdf
          </sc:servername>
          <sc:clientname>HIT_Q<sc:macro id="param-number"/>_userstudy.pdf</sc:clientname>
          <sc:errormessage>Do you have a PDF reader installed?</sc:errormessage>
        </sc:openfile>
      </sc:page>

      <sc:macro id="layoutquestion-page">
        <sc:macrodef id="param-question">
          <sc:macro id="param-basequestion">
            <sc:macrodef id="param-variation"><span class="varying"><sc:macro id="param-variationA"/></span></sc:macrodef>
          </sc:macro>
        </sc:macrodef>
        <sc:macrodef id="param-subletter">A</sc:macrodef>
      </sc:macro>
      <sc:macro id="layoutquestion-page">
        <sc:macrodef id="param-question">
          <sc:macro id="param-basequestion">
            <sc:macrodef id="param-variation"><span class="varying"><sc:macro id="param-variationB"/></span></sc:macrodef>
          </sc:macro>
        </sc:macrodef>
        <sc:macrodef id="param-subletter">B</sc:macrodef>
      </sc:macro>
    </sc:macrodef>

    <sc:page>
      <sc:name>instructions</sc:name>
      <sc:instructions>
        <html>
          <sc:macro id="layoutquestion-style"/>
          <h1>Instructions:</h1>
          <ul>
            <li>This HIT contains 9 two-part questions.</li>
            <li>Each two-part question is based on information in separate PDF file, which this software will open for you.</li>
            <li>The questions will be shown in this window.</li>
            <li>Please type your answers into the HIT form in your browser as you would for any other HIT.</li>
            <li>You can go back and forth between the questions with the "Next" and "Back" buttons below.</li>
            <li>At the end, you will be able to retrieve a confirmation code.</li>
          </ul>
