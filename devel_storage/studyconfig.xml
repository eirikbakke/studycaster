      <sc:whitelist>Quattro</sc:whitelist>
      <sc:whitelist>Mesa</sc:whitelist>
      <sc:whitelist>Spreadsheet Study Application</sc:whitelist>
      <sc:blacklist>Firefox</sc:blacklist>
      <sc:blacklist>Internet Explorer</sc:blacklist>
      <sc:blacklist>Chrome</sc:blacklist>
      <sc:blacklist>Safari</sc:blacklist>
      <sc:blacklist>Opera</sc:blacklist>
      <sc:blacklist>Thunderbird</sc:blacklist>
      <sc:blacklist>Outlook</sc:blacklist>
