<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping default-access="field" package="no.ebakke.studycaster.backend">
  <class name="Request" table="Requests">
    <id column="rid" name="id">
      <generator class="native"/>
    </id>
    <property column="rtime" name="time" type="timestamp"/>
    <property column="rtype" name="type"/>
  </class>
</hibernate-mapping>
